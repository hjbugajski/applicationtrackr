{
  "name": "applicationtrackr",
  "version": "1.3.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "prepare": "husky install",
    "prettier:check": "npx prettier --ignore-path .gitignore --check .",
    "prettier:write": "npx prettier --ignore-path .gitignore --write .",
    "stylelint": "npx stylelint **/*.scss --ignore-path .gitignore",
    "stylelint:fix": "npx stylelint **/*.scss --ignore-path .gitignore --fix",
    "bump": "bump ./src/app/settings/about/about.component.html",
    "bump:beta": "bump ./src/app/settings/about/about.component.html",
    "bump:beta:patch": "bump prepatch ./src/app/settings/about/about.component.html",
    "bump:beta:minor": "bump preminor ./src/app/settings/about/about.component.html",
    "bump:beta:major": "bump premajor ./src/app/settings/about/about.component.html",
    "bump:patch": "bump patch ./src/app/settings/about/about.component.html",
    "bump:minor": "bump minor ./src/app/settings/about/about.component.html",
    "bump:major": "bump major ./src/app/settings/about/about.component.html",
    "release": "commit-and-tag-version -s",
    "release:beta": "commit-and-tag-version -s --prerelease beta",
    "release:beta:custom": "commit-and-tag-version -s --prerelease beta --release-as",
    "release:beta:patch": "commit-and-tag-version -s --prerelease beta --release-as patch",
    "release:beta:minor": "commit-and-tag-version -s --prerelease beta --release-as minor",
    "release:beta:major": "commit-and-tag-version -s --prerelease beta --release-as major",
    "release:custom": "commit-and-tag-version -s --release-as",
    "release:patch": "commit-and-tag-version -s --release-as patch",
    "release:minor": "commit-and-tag-version -s --release-as minor",
    "release:major": "commit-and-tag-version -s --release-as major",
    "prerelease": "npm run bump:patch",
    "prerelease:beta": "npm run bump:beta:patch",
    "prerelease:beta:custom": "npm run bump:beta",
    "prerelease:beta:patch": "npm run bump:beta:patch",
    "prerelease:beta:minor": "npm run bump:beta:minor",
    "prerelease:beta:major": "npm run bump:beta:major",
    "prerelease:custom": "npm run bump",
    "prerelease:patch": "npm run bump:patch",
    "prerelease:minor": "npm run bump:minor",
    "prerelease:major": "npm run bump:major"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/cdk": "^15.0.0",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/fire": "^7.4.1",
    "@angular/forms": "^15.0.0",
    "@angular/material": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "rxjs": "^7.5.7",
    "tslib": "^2.4.1",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.0",
    "@angular-eslint/builder": "15.0.0",
    "@angular-eslint/eslint-plugin": "15.0.0",
    "@angular-eslint/eslint-plugin-template": "15.0.0",
    "@angular-eslint/schematics": "15.0.0",
    "@angular-eslint/template-parser": "15.0.0",
    "@angular/cli": "^15.0.0",
    "@angular/compiler-cli": "^15.0.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "commit-and-tag-version": "^10.1.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "stylelint": "^14.15.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-rational-order-fix": "^0.1.9",
    "stylelint-config-standard-scss": "^6.1.0",
    "typescript": "~4.8",
    "version-bump-prompt": "^6.1.0"
  },
  "commit-and-tag-version": {
    "scripts": {
      "postcommit": "git add .; git commit --amend --no-edit;"
    }
  }
}
